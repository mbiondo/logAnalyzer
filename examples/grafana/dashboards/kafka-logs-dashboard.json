{
  "id": null,
  "uid": "kafka-logs-dashboard",
  "title": "Kafka Logs Dashboard",
  "tags": ["loganalyzer", "kafka", "logs"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "Kafka Logs by Level",
      "type": "logs",
      "datasource": "Elasticsearch",
      "targets": [
        {
          "query": "_index:kafka-logs-2025.10.27 AND metadata.level:(ERROR OR WARN OR INFO)",
          "refId": "A",
          "metrics": [{ "type": "logs" }],
          "timeField": "@timestamp"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true,
        "enableLogDetails": true
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      }
    },
    {
